        xmlns:local="clr-namespace:Steclopaket"
        mc:Ignorable="d"
        Title="New order" Height="267.895" Width="338.97">
    <Grid Height="237" VerticalAlignment="Top">
        <GroupBox x:Name="groupBox" Header="Size " Margin="10,40,0,0" BorderBrush="Black" HorizontalAlignment="Left" Width="311" Height="103" VerticalAlignment="Top">
            <Grid Margin="0,0,-2,-2.96">
                <TextBox x:Name="tbWidth" Margin="86.04,12.96,15.96,0" TextWrapping="Wrap" Height="23" VerticalAlignment="Top"/>
                <Label x:Name="label" Content="Width, cm" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="71.04"/>
                <Label x:Name="label1" Content="High, cm" HorizontalAlignment="Left" Margin="10,40.96,0,0" VerticalAlignment="Top" Width="71.04" Height="25.96"/>
