            </Grid>
        </GroupBox>
        <Label x:Name="label2" Content="Window type" HorizontalAlignment="Left" Height="25" Margin="10,10,0,0" VerticalAlignment="Top" Width="82"/>
        <ComboBox x:Name="cbProduct" Margin="97,10,10,0" VerticalAlignment="Top" Height="25"/>
        <Button x:Name="btnSaveOrder" Content="Save" HorizontalAlignment="Left" Height="24" Margin="10,204,0,0" VerticalAlignment="Top" Width="71"/>
        <Label x:Name="label2_Copy" Content="Customer" HorizontalAlignment="Left" Margin="10,154,0,0" Width="82" Height="25" VerticalAlignment="Top"/>
        <ComboBox x:Name="cbCustomer" Margin="97,154,10,0" Height="25" VerticalAlignment="Top"/>
        <Button x:Name="btnCancel" Content="Cancel" Margin="86,204,154,0" VerticalAlignment="Top" Height="24" Click="btnCancel_Click"/>

    </Grid>
</Window>
