<Window x:Name="MaterChoice" x:Class="Steclopaket.MatSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Steclopaket"
        mc:Ignorable="d"
        Title="Material selection" Height="265.263" Width="337" Loaded="MaterChoice_Loaded" ResizeMode="NoResize">
    <Grid>
        <ListBox x:Name="lbMaterSelect" Margin="0,0,0,77"/>
        <Label x:Name="label" Content="Quantity" HorizontalAlignment="Left" Height="27" Margin="0,164,0,0" VerticalAlignment="Top" Width="57"/>
        <TextBox x:Name="tbQty" HorizontalAlignment="Left" Height="18" Margin="62,168,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="103" Text=" " PreviewTextInput="tbQty_PreviewTextInput" />
        <Button x:Name="btnOk" Content="OK" HorizontalAlignment="Left" Margin="10,201,0,0" VerticalAlignment="Top" Width="75" Click="btnOk_Click"/>
        <Button x:Name="tbnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="90,201,0,0" VerticalAlignment="Top" Width="75" Click="tbnCancel_Click"/>
    </Grid>
</Window>
